<p id="notice"><%= notice %></p>

<h1>Solicitud Estudiantes</h1>

<table>
  <thead>
    <tr>
      <th>Id solicitud</th>
      <th>Id estudiante</th>
      <th>Tipo solicitud</th>
      <th>Fecha</th>
      <th>Informacion adicional</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @solicitud_estudiantes.each do |solicitud_estudiante| %>
      <tr>
        <td><%= solicitud_estudiante.id_solicitud %></td>
        <td><%= solicitud_estudiante.id_estudiante %></td>
        <td><%= solicitud_estudiante.tipo_solicitud %></td>
        <td><%= solicitud_estudiante.fecha %></td>
        <td><%= solicitud_estudiante.informacion_adicional %></td>
        <td><%= link_to 'Show', solicitud_estudiante %></td>
        <td><%= link_to 'Edit', edit_solicitud_estudiante_path(solicitud_estudiante) %></td>
        <td><%= link_to 'Destroy', solicitud_estudiante, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Solicitud Estudiante', new_solicitud_estudiante_path %>
